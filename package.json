    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.13",
